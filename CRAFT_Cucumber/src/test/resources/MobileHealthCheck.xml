<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">	
<suite name="Mobile Health Checks" parallel="tests" verbose="1" thread-count="4" annotations="JDK">
    <test name="Login and Logout in iMFM app">
        <parameter name="ExecutionMode" value="SAUCELABS" />
        <parameter name="MobileToolName" value="APPIUM" />
        <parameter name="MobileExecutionPlatform" value="IOS" />
        <parameter name="DeviceName" value="CFG" />
        <classes>
            <class name="com.cucumbercraft.runners.MobileHealthChecks.RunneriMFM_HealthChecks" />
        </classes>
    </test>

    <test name="Login and Logout in SI iOS app">
        <parameter name="ExecutionMode" value="SAUCELABS" />
        <parameter name="MobileToolName" value="APPIUM" />
        <parameter name="MobileExecutionPlatform" value="IOS" />
        <parameter name="DeviceName" value="SI" />
        <classes>
            <class name="com.cucumbercraft.runners.MobileHealthChecks.RunnerSI_iOS_HealthChecks" />
        </classes>
    </test>

    <test name="Login and Logout in aMFM app">
        <parameter name="ExecutionMode" value="SAUCELABS" />
        <parameter name="MobileToolName" value="APPIUM" />
        <parameter name="MobileExecutionPlatform" value="ANDROID" />
        <parameter name="DeviceName" value="CFG" />
        <classes>
            <class name="com.cucumbercraft.runners.MobileHealthChecks.RunneraMFM_HealthChecks" />
        </classes>
    </test>

    <test name="Login and Logout in aSI app">
        <parameter name="ExecutionMode" value="SAUCELABS" />
        <parameter name="MobileToolName" value="APPIUM" />
        <parameter name="MobileExecutionPlatform" value="ANDROID" />
        <parameter name="DeviceName" value="SI" />
        <classes>
            <class name="com.cucumbercraft.runners.MobileHealthChecks.RunneraSIMobile_HealthChecks" />
        </classes>
    </test>

</suite>



